-const sections={'Inicio':"index.html",'Próximos conciertos':"conciertos.html",'Galería':"galería.html",'¿Quiénes somos?':"quienessomos.html",'¿Te gusta cantar?':"cantar.html",'Contacta':"contacta.html"}
footer.boxshadow
  div#copyright
    
    p 2022 
      i.fa.fa-copyright
  div#descripcion
    img(src="./img/marca2.png", alt="Logo y nombre MEGÓC")
